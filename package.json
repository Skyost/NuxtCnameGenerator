{
  "name": "nuxt-cname-generator",
  "version": "1.0.1",
  "description": "A Github Pages CNAME generator for Nuxt applications.",
  "repository": "Skyost/NuxtCnameGenerator",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "nuxi prepare && npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.3"
  }
}
